{
  "extends": [
    "wesbos"
  ],
  "rules": {
    "no-console": 2,    
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true,
        "printWidth": 120,
        "tabWidth": 2,
        "arrowParens": "avoid",
        "semi": false,
        "bracketSpacing": false,
      }
    ]
  },
}